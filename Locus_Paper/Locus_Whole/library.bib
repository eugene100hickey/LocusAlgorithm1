pmid = {25689872},
publisher = {Taylor {\&} Francis},
title = {{Safety of human papillomavirus vaccines: a review.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25689872 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4667712},
volume = {14},
