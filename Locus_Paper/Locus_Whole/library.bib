journal = {Neuroscience and biobehavioral reviews},
keywords = {Neurodevelopment,Neuronal protection,Parvalbumin,Perineuronal nets,Schizophrenia,Synaptic connectivity,Synaptic plasticity},
month = {sep},
pages = {85--99},
